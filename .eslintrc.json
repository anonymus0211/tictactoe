{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "rules": {
    "no-bitwise": 1,
    "eqeqeq": 1,
    "new-cap": 1,
    "no-undef": 1,
    "no-unused-vars": 1,
    "strict": 1,
    "no-empty": 1,
    "semi": 1,
    "wrap-iife": 1,
    "no-loop-func": 1,
    "no-caller": 1,
    "no-use-before-define": 1,
    "curly": 1,
    "quotes": [1, "single"]
  },
  "globals": {
    "define": true,
    "require": true,
    "exports": true,
    "module": true,
    "describe": true,
    "before": true,
    "beforeEach": true,
    "after": true,
    "afterEach": true,
    "it": true,
    "inject": true,
    "expect": true,
    "spyOn": true,
    "factory": true,
    "Request": true,
    "ErrorCodes": true
  }
}